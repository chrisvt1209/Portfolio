{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning",
        "OpenIddict.Server": "Warning",
        "OpenIddict.Core": "Warning",
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "RestrictedToMinimumLevel": "Verbose",
          "Path": "umbraco/CustomLogs/Verbose/verbose.json",
          "RollingInterval": "Day",
          "RetainedFileCountLimit": 3,
          "Formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      },
      {
        "Name": "File",
        "Args": {
          "RestrictedToMinimumLevel": "Debug",
          "Path": "umbraco/CustomLogs/Debug/debug.json",
          "RollingInterval": "Day",
          "RetainedFileCountLimit": 3,
          "Formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      },
      {
        "Name": "UmbracoFile",
        "Args": {
          "RestrictedToMinimumLevel": "Information",
          "FileSizeLimitBytes": 1073741824,
          "RollingInterval": "Day",
          "FlushToDiskInterval": null,
          "RollOnFileSizeLimit": false,
          "RetainedFileCountLimit": 31
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "umbracoDbDSN": "Data Source=|DataDirectory|/Umbraco.sqlite.db;Cache=Shared;Foreign Keys=True;Pooling=True",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.Sqlite"
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "6da36c5e-9556-4179-bb51-6c514820c1c2",
        "UseHttps": true,
        "ReservedPaths": "~/api,~/app_plugins/,~/install/,~/mini-profiler-resources/,~/umbraco/,~/callback,"
      },
      "ModelsBuilder": {
        "ModelsDirectory": "~/Models/Umbraco",
        "ModelsMode": "SourceCodeAuto",
        "ModelsNamespace": "PortfolioApp.Models.Umbraco"
      },
      "WebRouting": {
        "DisableRedirectUrlTracking": true,
        "UmbracoApplicationUrl": "https://localhost:44320"
      },
      "Content": {
        "Notifications": {
          "Email": "cvtalias@duck.com"
        }
      }
    }
  },
  "uSync": {
    "Settings": {
      "ImportOnFirstBoot": false,
      "ImportAtStartup": "None",
      "ExportOnSave": "All"
    }
  }
}