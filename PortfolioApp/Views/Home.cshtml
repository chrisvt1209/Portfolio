@using PortfolioApp.Models.Umbraco
@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<Home>
@{
    var sections = Model.Children()?.FirstOrDefault(x => x.ContentType.Alias == Sections.ModelTypeAlias);
    var navItems = sections?.Children() ?? Enumerable.Empty<IPublishedContent>();
}

@await Html.PartialAsync("~/Views/Partials/_Hero.cshtml", Model)

@foreach (var item in navItems)
{
    @await Html.PartialAsync($"~/Views/Partials/_{item.ContentType.Alias}.cshtml", item)
}

@await Html.PartialAsync("~/Views/Partials/_Footer.cshtml", Model)