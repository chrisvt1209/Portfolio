@using Microsoft.AspNetCore.Html
@using PortfolioApp.Models.Umbraco
@using Umbraco.Cms.Core.Models.Blocks
@using Umbraco.Cms.Web.Common.PublishedModels
@model IPublishedContent
@{
    var projectsSection = Model as Projects;
    if (projectsSection is null) return;

    var projectBlocks = projectsSection.Value<IEnumerable<BlockListItem>>(nameof(Projects.ProjectList));
    var allExperience = projectBlocks?
        .Select(block => block.Content as Project)
        .Where(project => project is not null)
        .Cast<Project>()
        .ToList()
        ?? new List<Project>();
}

<section class="section projects" id="projects">
    <div class="container">
        <div class="section-box">
            <div data-aos="fade-up" data-aos-duration="1000">
                <h3 class="section-title">@projectsSection.SectionTitle</h3>
            </div>
            <div class="d-flex project-container" data-aos="fade-up" data-aos-duration="1000">
                <div class="owl-carousel-0">
                    <div>
                        
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>